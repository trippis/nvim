{
  "coc.preferences.formatOnSaveFiletypes": [
    "javascript",
    "typescript",
    "json",
    "typescriptreact",
    "javascriptreact",
    "typescript.tsx",
    "css",
    "markdown",
    "html"
  ],
  "prettier.eslintIntegration": true,
